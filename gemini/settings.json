{
  "general": {
    "preferredEditor": "vscode",
    "previewFeatures": true,
    "enablePromptCompletion": true
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    },
    "allowedCommands": [
      "ls",
      "tree",
      "gh pr diff",
      "gh pr view",
      "git status"
    ]
  },
  "ui": {
    "theme": "Ayu Light",
    "footer": {
      "hideContextPercentage": false
    }
  },
  "output": {
    "format": "text"
  },
  "tools": {
    "shell": {
      "showColor": true
    }
  }
}